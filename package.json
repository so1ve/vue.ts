{
	"private": true,
	"version": "1.0.0-beta.9",
	"packageManager": "pnpm@10.20.0",
	"type": "module",
	"scripts": {
		"build": "pnpm -r run build",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"prepublishOnly": "nr build",
		"release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm publish -r --filter ./packages/*",
		"test": "vitest",
		"typecheck": "tsgo --noEmit"
	},
	"devDependencies": {
		"@antfu/ni": "^27.0.1",
		"@so1ve/eslint-config": "^3.11.2",
		"@so1ve/prettier-config": "^3.11.2",
		"@types/node": "^24.9.2",
		"@typescript/native-preview": "7.0.0-dev.20251030.1",
		"@vue-macros/test-utils": "^3.1.1",
		"bumpp": "^10.3.1",
		"changelogithub": "^13.16.1",
		"eslint": "^9.38.0",
		"prettier": "^3.6.2",
		"tsdown": "^0.15.11",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.5",
		"vue": "^3.5.22"
	}
}
